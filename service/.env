# SentraAI Service Configuration

# Server settings
PORT=5001
HOST=0.0.0.0
DEBUG=true

# Camera mode: simulated | live
# simulated: Uses sample_video.mp4 for testing
# live: Uses IP camera sources
CAMERA_MODE=simulated

# Camera sources (used when cameras are started)
ENTRY_CAMERA_SOURCE=../sample_videos/sample_video.mp4
EXIT_CAMERA_SOURCE=../sample_videos/sample_video.mp4

# Parking system backend URL
PARKING_API_URL=http://127.0.0.1:5000

# Detection settings
MIN_CONFIDENCE=0.6
DETECTION_COOLDOWN=3

# Auto entry/exit mode
# true: Automatically process entry/exit without confirmation
# false: Show confirmation popup for operator
AUTO_ENTRY_EXIT=false

# Frame processing
FRAME_SKIP=2
FRAME_WIDTH=640
FRAME_HEIGHT=480
JPEG_QUALITY=80
